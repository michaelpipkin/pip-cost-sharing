{
  "name": "pip-cost-sharing",
  "version": "3.0.5",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "kill-ports": "npx kill-port 9099 && npx kill-port 8080 && npx kill-port 5000 && npx kill-port 9199",
    "export-data": "rm seed-data -r -f && firebase emulators:export seed-data",
    "local-dev": "firebase emulators:start --import seed-data",
    "get-indexes": "firebase firestore:indexes",
    "build:prod": "npm --no-git-tag-version version patch && node ./setBuildDate.js && ng build --configuration=production",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "format": "prettier-eslint --write \"src/**/*.js\" \"src/**/*.ts\" \"src/**/*.scss\" \"src/**/*.html\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.9",
    "@angular/cdk": "^17.3.6",
    "@angular/common": "^17.3.9",
    "@angular/compiler": "^17.3.9",
    "@angular/core": "^17.3.9",
    "@angular/fire": "^17.0.1",
    "@angular/forms": "^17.3.9",
    "@angular/material": "^17.3.6",
    "@angular/platform-browser": "^17.3.9",
    "@angular/platform-browser-dynamic": "^17.3.9",
    "@angular/router": "^17.3.9",
    "bootstrap": "^5.3.3",
    "bootswatch": "^5.3.2",
    "firebase": "^10.8.0",
    "firebase-admin": "^12.0.0",
    "firebaseui": "^6.1.0",
    "moment": "^2.30.1",
    "replace-in-file": "^7.1.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.6",
    "@angular/cli": "^17.3.6",
    "@angular/compiler-cli": "^17.3.9",
    "@eslint/js": "^9.2.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/parser": "^5.0.1",
    "eslint": "^8.52.0",
    "globals": "^15.2.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "prettier": "^3.1.0",
    "prettier-eslint": "^16.3.0",
    "prettier-eslint-cli": "^8.0.1",
    "schematics-scss-migrate": "^2.3.17",
    "typescript": "^5.3.3",
    "typescript-eslint": "^7.8.0"
  }
}
