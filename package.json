    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.20.55",
    "@types/string-math": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^8.0.0-alpha.58",
    "@typescript-eslint/parser": "^8.0.0-alpha.58",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-unused-imports": "^4.0.1",
