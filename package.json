{
  "name": "pipsplit",
  "version": "1.6.40",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=local.dev.com",
    "kill-ports": "npx kill-port 4000 && npx kill-port 4500 && npx kill-port 5000 && npx kill-port 8080 && npx kill-port 9099 && npx kill-port 9150 && npx kill-port 9199",
    "export-data": "rm seed-data -r -f && firebase emulators:export seed-data",
    "local-dev": "firebase emulators:start --import seed-data",
    "get-indexes": "firebase firestore:indexes",
    "build:prod": "npm --no-git-tag-version version patch && node ./setBuildDate.js && ng build --configuration=production",
    "build:ci": "node ./setBuildDate.js && ng build --configuration=production",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "update-angular": "ng update @angular/cli @angular/core @angular/material @angular/cdk"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.4",
    "@angular/cdk": "^19.1.3",
    "@angular/common": "^19.1.4",
    "@angular/compiler": "^19.1.4",
    "@angular/core": "^19.1.4",
    "@angular/forms": "^19.1.4",
    "@angular/material": "^19.1.3",
    "@angular/platform-browser": "^19.1.4",
    "@angular/platform-browser-dynamic": "^19.1.4",
    "@angular/router": "^19.1.4",
    "bootstrap": "^5.3.3",
    "firebase": "^11.2.0",
    "moment": "^2.30.1",
    "replace-in-file": "^8.3.0",
    "string-math": "^1.2.2",
    "tslib": "^2.8.1",
    "url": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.6",
    "@angular/cli": "^19.1.6",
    "@angular/compiler-cli": "^19.1.4",
    "@hcaptcha/types": "^1.0.4",
    "@types/jasmine": "~5.1.5",
    "@types/node": "^22.13.1",
    "@types/string-math": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.14.0",
    "jasmine-core": "~5.5.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.4.2",
    "prettier-eslint": "^16.3.0",
    "prettier-eslint-cli": "^8.0.1",
    "schematics-scss-migrate": "^2.3.17",
    "typescript": "^5.7.3"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.34.4",
    "@rollup/rollup-win32-x64-msvc": "^4.34.4"
  }
}
