{
  "name": "pip-cost-sharing",
  "version": "1.7.5",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "kill-ports": "npx kill-port 9099 && npx kill-port 8080 && npx kill-port 5000 && npx kill-port 9199",
    "export-data": "rm seed-data -r -f && firebase emulators:export seed-data",
    "local-dev": "firebase emulators:start --import seed-data",
    "get-indexes": "firebase firestore:indexes",
    "build-prod": "npm --no-git-tag-version version patch && node ./setBuildDate.js && ng build --configuration=production",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.2.3",
    "@angular/cdk": "^17.2.1",
    "@angular/common": "^17.2.3",
    "@angular/compiler": "^17.2.3",
    "@angular/core": "^17.2.3",
    "@angular/fire": "^17.0.1",
    "@angular/forms": "^17.2.3",
    "@angular/material": "^17.2.1",
    "@angular/platform-browser": "^17.2.3",
    "@angular/platform-browser-dynamic": "^17.2.3",
    "@angular/router": "^17.2.3",
    "bootstrap": "^5.3.3",
    "bootswatch": "^5.3.2",
    "firebase": "^10.8.0",
    "firebase-admin": "^12.0.0",
    "firebaseui": "^6.1.0",
    "moment": "^2.30.1",
    "replace-in-file": "^7.1.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
